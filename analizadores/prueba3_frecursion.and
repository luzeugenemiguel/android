programa prueba3_frecursion:

funcion entero factorial(entero a){
	escribe("a=".a);
	si(a < 2){
		regresa 1;
	}sino{
		regresa a * factorial(a-1);
	}
}

principal{
	entero a;
	entero i = 1;
	entero lectura;
	escribe("Â¿hasta que numero calculo el factorial?");
	lectura = lee();
	mientras(i <= lectura){
		
		a = factorial(i);
		escribe(a);
		i++;
	}
}
